<!--begin::Company Work Place Modal-->
<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.14.0-beta2/dist/css/bootstrap-select.min.css">

<!-- Latest compiled and minified JavaScript -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.14.0-beta2/dist/js/bootstrap-select.min.js"></script>

<!-- (Optional) Latest compiled and minified JavaScript translation files -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.14.0-beta2/dist/js/i18n/defaults-*.min.js"></script>

<div class="modal fade " id="kt_modal_companyworkplace" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" style="display: none;" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="note">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">WorkPlace</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label class="form-control-label">Company Name</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.company_name" />

                        </div>
                        <div class="form-group">
                            <label class="form-control-label">Address1</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.address1" />

                        </div>
                        <div class="form-group">
                            <label class="form-control-label">Address2</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.address2" />

                        </div>
                        <div class="form-group">
                            <label class="form-control-label">Town/City</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.town" />

                        </div>
                        <div class="form-group">
                            <label class="form-control-label">County</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.county" />

                        </div>

                        <div class="form-group">
                            <label class="form-control-label">Postcode</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.post_code" />

                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="form-group">
                            <label class="form-control-label">Manager Name</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.wp_name" />
                        </div>
                        <div class="form-group">
                            <label class="form-control-label">Employer Email  Address</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.employee_email" />
                        </div>
                        <div class="form-group">
                            <label class="form-control-label">Phone Number</label>
                            <input type="text" class="form-control" v-model="companyWorkPlaces.phone_number" />
                        </div>
                        <div class="form-group">
                            <label class="form-control-label">Job Title</label>
                            <select class="form-control select2"  id="jobTitleId"  v-model="companyWorkPlaces.job_title">
                                <option v-for="notesCat in JobTitles" v-bind:value="notesCat.id" v-text="notesCat.description"></option>
                            </select>
                        </div>
                        <div class="form-group">
                            <label> Business Type</label>
                            <select class="form-control select2" id="bussTypeId" v-model="companyWorkPlaces.business_type">
                                <option value=""></option>
                                <option value="Adult Residential Home">Adult Residential Home</option>
                                <option value="Community Healthcare Services">Community Healthcare Services</option>
                                <option value="Domiciliary Care Agency">Domiciliary Care Agency</option>
                                <option value="Supported Living Services">Supported Living Services</option>
                                <option value="Rehabilitation Services">Rehabilitation Services</option>
                                <option value="Semi-independent">Semi-independent</option>
                                <option value="Foster Care Agency">Foster Care Agency</option>
                                <option value="Children’s Residential Home">Children’s Residential Home</option>
                                <option value="School with Residential">School with Residential</option>
                                <option value="CYP Non-Residential ">CYP Non-Residential </option>
                                <option value="Live-in Carer / Services in your home">Live-in Carer / Services in your home</option>
                                <option value="Nursing Home">Nursing Home</option>
                                <option value="Agency">Agency</option>
                                <option value="Extra Care Housing">Extra Care Housing</option>
                                <option value="GP Services">GP Services</option>
                                <option value="Day Care Centre">Day Care Centre</option>
                                <option value="Hospices">Hospices</option>
                                <option value="Clinics">Clinics</option>

                            </select>
                            @*@(Html.Kendo().DropDownList()
                                .Name("ddl_BusinessType")
                                .HtmlAttributes(new { @class = "form-control" })

                                .OptionLabel("Select Business Type")
                                .BindTo(new string[] { "Adult Residential Home", "Community Healthcare Services", "Domiciliary Care Agency", "Supported Living Services", "Rehabilitation Services" })
                                .Filter(FilterType.StartsWith)
                                )*@
                        </div>

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                <button type="submit" v-on:click="submitWorkPlace" class="btn btn-success">Submit</button>
            </div>
        </div>
    </div>
</div>
<script>

</script>
